# Production Environment Configuration
NODE_ENV=production
PORT=3000
SESSION_SECRET=your-production-session-secret-here

# Data Protection
PRESERVE_DATA=true
PRODUCTION_DATA_DIR=/app/data

# Database Safety
DB_BACKUP_ENABLED=true
DB_MIGRATION_SAFE=true

# Security
CORS_ORIGIN=https://your-production-domain.com
TRUST_PROXY=true

# Logging
LOG_LEVEL=info
