# Staging Environment Configuration
NODE_ENV=staging
PORT=3000
SESSION_SECRET=your-staging-session-secret-here

# Data Protection
PRESERVE_DATA=true
STAGING_DATA_DIR=./data-staging

# Database Safety
DB_BACKUP_ENABLED=true
DB_MIGRATION_SAFE=true

# Security
CORS_ORIGIN=https://your-staging-domain.com
TRUST_PROXY=true

# Logging
LOG_LEVEL=debug
