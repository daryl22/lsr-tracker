{
  "name": "lsr-tracker",
  "version": "1.0.0",
  "description": "System tracker with user registration, daily km & hours input, and file uploads",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "pull-data": "node pull-data.js",
    "backup": "node backup-local.js",
    "find-live": "node find-live.js",
    "deploy": "node deploy-safe.js",
    "deploy:dev": "set NODE_ENV=development && node deploy-safe.js",
    "deploy:staging": "set NODE_ENV=staging && node deploy-safe.js",
    "deploy:prod": "set NODE_ENV=production && node deploy-safe.js",
    "deploy:ps": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:ps:dev": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Environment development",
    "deploy:ps:staging": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Environment staging",
    "deploy:ps:prod": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Environment production",
    "staging": "node deploy-staging.js",
    "staging:start": "node -e \"process.env.NODE_ENV='staging'; process.env.PORT='3001'; import('./src/app.js')\"",
    "staging:dev": "NODE_ENV=staging PORT=3001 nodemon src/app.js",
    "pull:live": "node pull-live-complete.js",
    "pull:data": "node pull-data.js",
    "pull:files": "node pull-files-only.js",
    "pull:files-manual": "node pull-files-manual.js",
    "pull:files-simple": "node pull-files-simple.js",
    "pull:production": "node pull-production.js",
    "pull:production-complete": "node pull-production-complete.js",
    "migrate:sqlite-to-postgresql": "node migrate-sqlite-to-postgresql.js",
    "migrate:staging-to-postgresql": "node migrate-staging-to-postgresql.js",
    "migrate:production-to-postgresql": "node migrate-production-to-postgresql.js",
    "pull:live-and-migrate": "node pull-live-and-migrate-to-postgresql.js",
    "pull:railway-sqlite": "node pull-railway-sqlite-data.js",
    "pull:railway-guide": "node pull-railway-data-guide.js",
    "pull:railway-step": "node pull-railway-step-by-step.js",
    "pull:railway-complete": "node pull-railway-complete.js",
    "test:railway-pull": "node test-railway-pull-local.js",
    "execute:railway-pull": "node execute-railway-pull.js",
    "run:railway-pull": "node run-railway-pull.js",
    "install:postgresql": "node install-postgresql-client.js",
    "alternative:railway-pull": "node alternative-railway-pull.js",
    "fix:postgresql": "node fix-postgresql-connection.js",
    "pull:live-domain": "node pull-from-live-domain.js",
    "migrate:live-to-postgresql": "node migrate-live-to-postgresql.js",
    "railway:guide": "node railway-cli-guide.js",
    "fix:railway-db": "node fix-railway-database-access.js",
    "alternative:data-pull": "node alternative-data-pull-methods.js",
    "setup:postgresql": "node setup-local-postgresql.js",
    "setup:railway": "node setup-railway-postgresql.js",
    "setup:free": "node setup-free-postgresql.js",
    "setup:connection": "node setup-postgresql-connection.js",
    "demo:migration": "node demo-migration.js"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.12.6",
    "bcrypt": "^5.1.1",
    "connect-sqlite3": "^0.9.15",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
